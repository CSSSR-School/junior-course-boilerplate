{
  "name": "csssr-school-course",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "1.5.0",
    "classnames": "2.2.6",
    "csssr-school-utils": "git+https://github.com/CSSSR-School/junior-course-utils.git",
    "lodash.isequal": "4.5.0",
    "lodash.memoize": "4.1.2",
    "react": "16.12.0",
    "react-addons-shallow-compare": "15.6.3",
    "react-dom": "16.12.0",
    "react-redux": "7.2.2",
    "react-scripts": "3.2.0",
    "redux": "4.0.5",
    "reselect": "4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint:js": "eslint \"src/**/*.{js, jsx}\" --cache --fix"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-plugin-prettier": "3.1.2",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1"
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
